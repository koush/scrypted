      "google-auth-library": "^7.9.2",
      "lodash": "^4.17.21",
      "url-parse": "^1.5.1"
   },
   "devDependencies": {
      "@scrypted/common": "file:../../common",
      "@scrypted/sdk": "file:../../sdk",
      "@types/debug": "^4.1.5",
      "@types/lodash": "^4.14.168",
      "@types/url-parse": "^1.4.3"
   },
   "version": "0.0.51"
